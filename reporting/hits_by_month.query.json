{
  "aggs": {
    "month": {
      "date_histogram": {
        "field": "@apache_timestamp",
        "interval": "1M",
        "time_zone": "America/New_York",
        "min_doc_count": 1,
        "format": "yyyyMM"
      }
    }
  },
  "size": 0,
  "docvalue_fields": [
    {
      "field": "@apache_timestamp",
      "format": "date_time"
    },
    {
      "field": "@timestamp",
      "format": "date_time"
    }
  ],
  "query": {
    "bool": {
      "must": [
        {
          "query_string": {
            "query": "probably_a_bot:false && (response:200 || response:304) && internal_IP:false && !(clientip:127.0.0.1 || clientip:\"::1\" || clientip:206.108.125.163)",
            "analyze_wildcard": true,
            "default_field": "*"
          }
        },
        {
          "range": {
            "@apache_timestamp": {
                "gte": _FROM_DATE,
                "lte": _TO_DATE,
                "format": "basic_date"
            }
          }
        }
      ]
    }
  }
}
